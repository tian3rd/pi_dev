# This DataBase sets up the ADC channels for ADC4
# ACROMAG XT1221-000 8 Chanel Diff ADC module
# 
# # - Channels read
# 0 - A3:ACROMAG_ADC4_INPUT0
# 1 - A3:ACROMAG_ADC4_INPUT1
# 2 - A3:ACROMAG_ADC4_INPUT2
# 3 - A3:ACROMAG_ADC4_INPUT3
# 4 - A3:ACROMAG_ADC4_INPUT4
# 5 - A3:ACROMAG_ADC4_INPUT5
# 6 - A3:ACROMAG_ADC4_INPUT6
# 7 - A3:ACROMAG_ADC4_INPUT7
#
# Channels calculated from read channels
# 0 -> A1:ISC-FSS_LASER_B_PCM_RB
# 1 -> A1:ISC-FSS_LASER_B_FAST_RB
# 2 -> A1:ISC-FSS_LASER_C_SLOW_RB
# 
# 3 -> A1:ISC-FSS_LASER_C_LOM_RB
# 4 -> A1:ISC-FSS_LASER_C_MIXR_RB
# 5 -> A1:ISC-FSS_LASER_C_PCM_RB
#
# 6 -> A1:ISC-FSS_LASER_C_FAST_RB
# 7 -> SPARE
#
# ANU Torpedo Experiment
# Joshua McCann - 13 June 2018
#
######################################################################
###############		ISC-FSS LASER B		######################
record(ai, "A3:ACROMAG_ADC4_INPUT0")
{
		field(SCAN,".1 second")
		field(FLNK,"A1:ISC-FSS_LASER_B_PCM_RB.PROC")
		field(DTYP,"asynInt32")
		field(INP, "@asynMask(ADC4_Reg 0 -16)MODBUS_DATA")
		field(EGUL,"-30000")
		field(EGUF,"30000")
		field(LINR,"LINEAR")
}
record(calc, "A1:ISC-FSS_LASER_B_PCM_RB")
{
#        field(SCAN,".1 second")
        field(INPA,"A3:ACROMAG_ADC4_INPUT0.VAL NPP MS")
		field(DESC,"Convert LASER_B_PCM_RB count to Volt")
        field(CALC,"A*1.325/(0.12076*30000)")
		field(HOPR,"10")
		field(LOPR,"-10")
		field(PREC, "5")
}
