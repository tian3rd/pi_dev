# This DataBase sets up the ADC channels for ADC0
# ACROMAG XT1221-000 8 Chanel Diff ADC module
# 
# # - Channels read
# 0 - A3:ACROMAG_ADC0_INPUT0
# 1 - A3:ACROMAG_ADC0_INPUT1
# 2 - A3:ACROMAG_ADC0_INPUT2
# 3 - A3:ACROMAG_ADC0_INPUT3
# 4 - A3:ACROMAG_ADC0_INPUT4
# 5 - A3:ACROMAG_ADC0_INPUT5
# 6 - A3:ACROMAG_ADC0_INPUT6
# 7 - A3:ACROMAG_ADC0_INPUT7
#
# Channels calculated from read channels
# 0 -> A1:PEM-LAB_STS2_A_MASS_POS_U
# 1 -> A1:PEM-LAB_STS2_A_MASS_POS_V
# 2 -> A1:PEM-LAB_STS2_A_MASS_POS_W
# 
# 3 -> A1:PEM-LAB_STS2_B_MASS_POS_U
# 4 -> A1:PEM-LAB_STS2_B_MASS_POS_V
# 5 -> A1:PEM-LAB_STS2_B_MASS_POS_W
#
# 6 -> A1:PEM-LAB_STS2_C_MASS_POS_U
# 7 -> A1:PEM-LAB_STS2_C_MASS_POS_V
#
# ANU Torpedo Experiment
# Bram Slagmolen - 13 August 2017
#
######################################################################
###############		STS-2 Unit A 		######################
record(ai, "A3:ACROMAG_ADC0_INPUT0")
{
		field(SCAN,".1 second")
		field(FLNK,"A1:PEM-LAB_STS2_A_MASS_POS_U.PROC")
		field(DTYP,"asynInt32")
		field(INP, "@asynMask(ADC0_Reg 0 -16)MODBUS_DATA")
		field(EGUL,"-30000")
		field(EGUF,"30000")
		field(LINR,"LINEAR")
}
record(calc, "A1:PEM-LAB_STS2_A_MASS_POS_U")
{
#        field(SCAN,".1 second")
        field(INPA,"A3:ACROMAG_ADC0_INPUT0.VAL NPP MS")
		field(DESC,"Convert B_A POS-U count to Volt")
        field(CALC,"A*1.325/(0.12076*30000)")
		field(HOPR,"10")
		field(LOPR,"-10")
		field(PREC, "5")
}
########################################################################
