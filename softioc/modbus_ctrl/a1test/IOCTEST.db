record(ai, "A3:ACROMAG_TEMP_INPUT0")
{
	field(SCAN, ".1 second")
	field(FLNK, "A3:ACROMAG_TEMP_INPUT0_C.PROC")
	field(DTYP, "asynInt32")
	field(INP, "@asynMask(TEMP_Reg 0 -16)MODBUS_DATA")
}
record(calc, "A3:PEM-FRAME_TEMP_C")
{
	field(INPA,"A3:ACROMAG_TEMP_INPUT0.VAL NPP MS")
	field(DESC,"Convert Frame Temp resistance to Deg C")
#	field(CALC,"(-100 * 3.9083E-3 + SQR( 100^2* (3.9083E-3)^2 - 4*100*-5.775E-7*(100-A)) ) / (2*100*-5.775E-7)")
#	1 / (2*100*-5.775E-7) = -8658.008
	field(CALC,"-8658*(-100*3.9083E-3 + SQR(100^2* (3.9083E-3)^2 - 4*100*-5.775E-7*(100-A)))")
	field(EGU,"DegC")
	field(PREC,"3")
	field(HOPR,"500")
	field(LOPR,"0")
}
